{

  "manifest_version": 2,
  "name": "PocketCaster",
  "short_name": "PocketCaster",
  "description": "",
  "version": "2.0.1",
  "minimum_chrome_version": "38",
  "permissions": [ 
    "https://pocketcasts.com/", 
    "https://*.pocketcasts.com/", 
    "https://play.pocketcasts.com/", 
    "webview", 
    "notifications", 
    "https://www.google-analytics.com/",
    "storage" ],

  "author": "jomurgel",
  "description": "Needed a chrome app for Pocket Casts. Tabs are so last year.",
  "icons": { 
    "16": "assets/icon_16.png", 
    "48": "assets/icon_48.png", 
    "96": "assets/icon_96.png", 
    "128": "assets/icon_128.png", 
    "256": "assets/icon_256.png"
  },

  "key": "",

  "app": {
      "background": {
        "scripts": ["pcasts.js"]
    }
  },

  "commands": {
    "pause": {
      "suggested_key": {
        "default": "MediaPlayPause"
      },
      "description": "Play/Pause",
      "global": true
    },
    "next": {
      "suggested_key": {
        "default": "MediaNextTrack"
      },
      "description": "Next Song",
      "global": true
    },
    "previous": {
      "suggested_key": {
        "default": "MediaPrevTrack"
      },
      "description": "Previous Song",
      "global": true
    }
  }
}
